<?xml version="1.0" encoding="UTF-8"?>
<!-- oracle import configuration Created: 08.2012 : Gunther Pippèrr (c) http://www.pipperr.de -->
<import version="0.2">

	<!-- Database connection -->
	<db_settings>
		<tns_alias>gpi</tns_alias>
		<username password="" encrypt="true" certificate="false">scott</username>
		<!-- Language of messages - charset -->
		<nls_lang>AMERICAN_AMERICA.UTF8</nls_lang>
		<!-- where we can find the .net library of oracle -->
		<dot_net_orcle_home>d:\oracle\product\11.2.0.3\dbhome_1</dot_net_orcle_home>
    </db_settings>
	
  <xls_file>
    <!--  Name of the import job -->
    <job_description>Import_new_employes</job_description>
    <!-- File to import -->
    <file_location>D:\OraPowerShellCodePlex\test\test_data_set.xlsx</file_location>
    
    
    <!-- Worksheet (tab) in the file to import -->
    <worksheet_number>1</worksheet_number>
    <!-- list of import Columns -->
    <row_description>
      <!-- start from which row position  1 is the first one -->
      <start_row>2</start_row>
      <!-- read until this position -1 until end -->
      <max_row>-1</max_row>
	  <!-- how often we should commit the new data -->
	  <commit_count>10</commit_count>
      <!-- first column -->
      <column>
        <!-- name of the column for better readness -->
        <name>employee</name>
        <!-- position of the row -->
        <!-- attribute execl_name is the abcd name of the column -->
        <position excel_name="A">1</position>
        <!-- transform the data to the target -->
        <transform_rule>upper</transform_rule>
        <!-- write to database -->
        <schema>scott</schema>
        <tablename>emp</tablename>
        <tab_column>name</tab_column>
      </column>
      <!-- second column -->
      <column>
        <!-- read from xls -->
        <name>age</name>
        <position>2</position>
        <transform_rule trans_rule="data" trans_pattern="dd.mm.yyyy" db_obj_name="" >date</transform_rule>
        <!-- write to database -->
        <schema>scott</schema>
        <tablename>emp</tablename>
        <tab_column>age</tab_column>
      </column>
      <!-- 3. column -->
      <column>
        <!-- read from xls -->
        <name>salary</name>
        <position>3</position>
        <transform_rule>number</transform_rule>
        <!-- write to database -->
        <schema>scott</schema>
        <tablename>dept</tablename>
        <tab_column>sal</tab_column>
	  </column>
	  <column>
        <!-- dummy column - autogenerated -->
        <name>salary2</name>
        <position>-1</position>
        <transform_rule trans_rule="sequence"  trans_pattern="" db_obj_name="seq_dept_id">auto_column</transform_rule>
        <!-- write to database -->
        <schema>scott</schema>
        <tablename>dept</tablename>
        <tab_column>sal</tab_column>
		</column>
    </row_description>
  </xls_file>
  <!-- next file -->
</import>