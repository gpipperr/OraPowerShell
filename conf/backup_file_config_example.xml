		<vss_driveletter>y:</vss_driveletter>
		<!-- use VSS feature -->
		<use_vss>false</use_vss>
		<!-- Definitions of the folder structure -->
	    <folder>
			<source>D:\OraPowerShellCodePlex</source>
			<target>\\Fritz!nas\fritz.nas\JetFlash-Transcend64GB-01\jupiter\OraPowerShellCodePlex</target>
			<!-- if the share needs to be connected! -->
			<target_share>\\Fritz!nas\fritz.nas</target_share>
			<!-- Robocopy feature -->
			<robocopy_feature><![CDATA[/S /W:0 /R:0 /B]]></robocopy_feature>
		</folder>			
	</volume>
	<volume>
		<!-- Name of the volume -->
		<name>CDATA</name>
