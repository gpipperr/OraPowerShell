	<oracle_home>
		<!-- Path to the Oracle Home -->
		<path>D:\oracle\11.2.0.3\grid</path>
		<!-- Name of the Oracle Home -->
		<oraname>ASM</oraname>
		<!-- add to selection list -->
		<enabled>false</enabled>
		<!-- SID List of databases in this home -->
